{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "functions": {
    "api/health.ts": {
      "runtime": "@vercel/node"
    },
    "api/test.ts": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}
